{
    "stryker-config":
    {
        "reporters": [
            "progress",
            "html"
        ],
        "log-level": "info",
        "log-file":true,
        "timeout-ms": 10000,
        "project-file": "RestClient.Net.UnitTests.csproj",
        "max-concurrent-test-runners": 4,
        "threshold-high": 80,
        "threshold-low": 70,
        "threshold-break": 60,
        "mutation-level": "Complete",
        "excluded-mutations": [
        ],
        "ignore-methods": [
            "*Log*",
            "ToString",
            "*HashCode*",
            "*ConfigureAwait*"
        ]
    }
}